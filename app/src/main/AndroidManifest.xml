<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yashraj.videomeet">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/rectangle_1_9"
        android:label="@string/app_name"
        android:roundIcon="@drawable/rectangle_1_9"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.VideoChatActivity"></activity>
        <activity android:name=".Activity.CallingActivity" />
        <activity android:name=".Activity.ProfileActivity" />
        <activity android:name=".Activity.FindPeopleActivity" />
        <activity android:name=".Activity.NotificationActivity" />
        <activity android:name=".Activity.SettingActivity" />
        <activity android:name=".Activity.ContextActivity" />
        <activity android:name=".Activity.RegistrationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>